<div class="container-fluid">
    <div class="row">
        <div class="col-sm-10">
            {{#each tickets}}
            <div class='col-sm-8'>
                <div class="panel panel-{{prioClass prio}}">
                    <div class="panel-heading">
                        <h4 class='panel-title'>{{titolo}}</h4>
                    </div>
                    <div class="panel-body">
                        <h6>Ultimo evento:</h6>
                        <p>
                            {{ultimoEvento eventi}}
                        </p>
                        <hr>
                        <p>{{prio}}</p>
                    </div>
                    <div class='panel-footer'>
                        {{!--
                        <a class='btn btn-default' href='#'>+1</a> --}}
                        <a class="btn btn-info" href="/tickets/{{_id}}">Apri</a>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        <div class="col-sm-2">
            <ul class="nav nav-pills">
                <li role="presentation" class="active">
                    <a href="#">Home</a>
                </li>
                <li role="presentation">
                    <a href="#">Profile</a>
                </li>
                <li role="presentation">
                    <a href="#">Messages</a>
                </li>
            </ul>
        </div>
    </div>

</div>
</div>